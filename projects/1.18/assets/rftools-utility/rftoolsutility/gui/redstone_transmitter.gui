window {
    size(168,44)

    action('reset','reset')

    panel('','vertical') {
        bgthickness(2)
        textfield('name') {
            channel('name')
            desired(160,16)
            tooltips('自定义此红石', '频道的名称')
        }
        button('reset','重置') {
            channel('reset')
            desired(160,16)
            tooltips('重置频道')
        }
    }
}
