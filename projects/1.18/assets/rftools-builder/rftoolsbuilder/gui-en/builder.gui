    window {
        size(180,152)

        action('restart','restart')

        bind('redstone','rsmode')
        bind('silent','silent')
        bind('entities','entities')
        bind('wait','wait')
        bind('loop','loop')
        bind('hilight','hilight')
        bind('support','support')
        bind('mode', 'mode')
        bind('rotate', 'rotate')

        panel('','positional') {
            bg1('rftoolsbuilder:textures/gui/builder.png')
            energybar('energybar') {
                hint(10,6,8,59)
                showtext(false)
            }
            panel('','positional') {
                hint(19,6,61,59)
                bgthickness(-2)
                bgfilled1(-7631989)
                choicelabel('mode') {
                    channel('mode')
                    hint(9,4,42,14)
                    enableon('validcard')
                    tooltips('Set the building mode')
                    choices {
                        choice('Copy') {
                            tooltips('Copy from space chamber to here','Chest on top or below with materials')
                        }
                        choice('Move') {
                            tooltips('Move from space chamber to here')
                        }
                        choice('Swap') {
                            tooltips('Swap space chamber contents with here')
                        }
                        choice('Back') {
                            tooltips('Move back from here to space chamber')
                        }
                        choice('Collect') {
                            tooltips('Collect items in space chamber','Items will go to chest on top or below')
                        }
                    }
                }
            imagechoicelabel('silent') {
                channel('silent')
                hint(4,18,17,17)
                tooltips('Suppress the placement/breaking sound','when moving blocks')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(176,48,256,256)
                choices {
                    choice('off') {
                        uv(176,48)
                        tooltips('Moving blocks make sound')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(160,48)
                        tooltips('Block sounds are muted')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('support') {
                channel('support')
                hint(22,18,17,17)
                tooltips('Use supporting blocks when moving.','Useful for liquids, gravel, ...')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(112,48,256,256)
                choices {
                    choice('off') {
                        uv(112,48)
                        tooltips('Support/preview mode disabled')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(96,48)
                        tooltips('Support/preview mode enabled')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('entities') {
                channel('entities')
                hint(4,37,17,17)
                tooltips('Move entities')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(144,48,256,256)
                choices {
                    choice('off') {
                        uv(144,48)
                        tooltips('Entities are not moved')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(128,48)
                        tooltips('Entities are moved')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('loop') {
                channel('loop')
                hint(22,37,17,17)
                tooltips('Loop mode')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(208,48,256,256)
                choices {
                    choice('off') {
                        uv(208,48)
                        tooltips('Do a single run and stop')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(192,48)
                        tooltips('Keep running with redstone signal')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('wait') {
                hint(40,18,17,17)
                tooltips('Wait mode')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(96,80,256,256)
                choices {
                    choice('off') {
                        uv(112,80)
                        tooltips('Don\'t wait on a position if','the operation is not possible')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(96,80)
                        tooltips('If the operation is not possible','wait on the current position')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('hilight') {
                channel('hilight')
                hint(40,37,17,17)
                tooltips('Hilight mode')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(144,80,256,256)
                choices {
                    choice('off') {
                        uv(144,80)
                        tooltips('No hilighting')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(128,80)
                        tooltips('Visually hilight the position','the Builder is working on')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
        }
        panel('','positional') {
            hint(128,6,44,59)
            bgthickness(-2)
            bgfilled1(-7631989)
            choicelabel('rotate') {
                channel('rotate')
                hint(4,4,36,14)
                enableon('validcard')
                tooltips('Set the horizontal rotation angle')
                choices {
                    choice('0')
                    choice('90')
                    choice('180')
                    choice('270')
                }
            }
            imagechoicelabel('anchor0') {
                channel('anchor')
                hint(4,37,17,17)
                tooltips('Set the anchor where you want to','place the blocks in front of the','builder')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(96,64,256,256)
                choices {
                    choice('off') {
                        uv(112,64)
                        tooltips('Builder at south west corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(96,64)
                        tooltips('Builder at south west corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('anchor1') {
                channel('anchor')
                hint(23,37,17,17)
                tooltips('Set the anchor where you want to','place the blocks in front of the','builder')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(144,64,256,256)
                choices {
                    choice('off') {
                        uv(144,64)
                        tooltips('Builder at south east corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(128,64)
                        tooltips('Builder at south east corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('anchor2') {
                channel('anchor')
                hint(4,18,17,17)
                tooltips('Set the anchor where you want to','place the blocks in front of the','builder')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(176,64,256,256)
                choices {
                    choice('off') {
                        uv(176,64)
                        tooltips('Builder at north west corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(160,64)
                        tooltips('Builder at north west corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
            imagechoicelabel('anchor3') {
                channel('anchor')
                hint(23,18,17,17)
                tooltips('Set the anchor where you want to','place the blocks in front of the','builder')
                border(true)
                image('rftoolsbase:textures/gui/guielements.png')
                dim(208,64,256,256)
                choices {
                    choice('off') {
                        uv(208,64)
                        tooltips('Builder at north east corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                    choice('on') {
                        uv(192,64)
                        tooltips('Builder at north east corner')
                        image('rftoolsbase:textures/gui/guielements.png')
                    }
                }
            }
        }
        button('','?') {
            channel('cardgui')
            hint(83,12,13,12)
            tooltips('Click to open the card gui')
        }
        button('level','Y: stop') {
            channel('restart')
            hint(81,31,45,13)
            tooltips('Current level the builder is at','�ePress to restart!')
        }
        imagechoicelabel('redstone') {
            hint(106,46,16,16)
            image('rftoolsbase:textures/gui/guielements.png')
            dim(32,0,256,256)
            choices {
                choice('Ignored') {
                    uv(0,0)
                    tooltips('Redstone mode:','Ignored')
                    image('rftoolsbase:textures/gui/guielements.png')
                }
                choice('Off') {
                    uv(16,0)
                    tooltips('Redstone mode:','Off to activate')
                    image('rftoolsbase:textures/gui/guielements.png')
                }
                choice('On') {
                    uv(32,0)
                    tooltips('Redstone mode:','On to activate')
                    image('rftoolsbase:textures/gui/guielements.png')
                }
            }
        }
    }
}
